# Make the CURSOR package.

$checkout libcur.a lib$
$update   libcur.a
$checkin  libcur.a lib$
$exit

libcur.a:
	# $set xflags = "$(xflags) -qfx"

	giotr.x		gtr.com gtr.h <config.h> <gio.h> <gki.h> <xwhen.h>
