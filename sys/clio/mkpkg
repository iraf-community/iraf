# CLIO portion of the system library.

$checkout libsys.a lib$
$update   libsys.a
$checkin  libsys.a lib$
$exit

libsys.a:	
	clcache.x	clio.com <clio.h> <ctype.h>
	clclose.x	<config.h>
	clcmd.x		<clset.h>
	clcmdw.x	<clset.h>
