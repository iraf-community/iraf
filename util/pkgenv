#!/bin/bash
#
#  PKGENV -- Initialize the package environment


/bin/rm -f .installed .zzsetenv.def

files=`cat .repo_pkgs`
for p in ${files[@]} ; do

    if [ -e $p/.installed ]; then
	cat $p/.installed 			>> .installed
	/bin/echo "re$p	="`pwd`"/$p/"		>> .zzsetenv.def
    fi
done
/bin/echo "keep"				>> .zzsetenv.def
