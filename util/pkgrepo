#!/bin/bash
#
#  PKGREPO - Get the repository base URI, either from the environment
#  variable 'IRAF_REPO' or the default.

base_url="https://gitlab.com/nsf-noirlab/csdc/usngo/iraf/"

if [ -z "$IRAF_REPO" ]; then
   /bin/echo ${base_url}"iraf-distros/-/raw/main/iraf-v218/extern"
else
   /bin/echo $IRAF_REPO
fi

exit 0
