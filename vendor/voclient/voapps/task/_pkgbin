#include <stdio.h>
#include <stdlib.h>


int main (int argc, char *argv[])
{

    /* Process arguments.
     */
    for (i=1; i < argc; i++) {
        switch (argv[i]) {
	case 'h':			// help 
	case 'd':			// debug 
	case 'v':			// verbose 

	case 'a':			// package attributes
	case 't':			// package task list
	case 'p':			// task parameters ("-p <task>")

	case 'S':			// task parameters ("-p <task>")
	default:
	    taskName = argv[i]		// execute task ....
        }
    }


}
