	:DD=apl,tmp$sgk,!{ sgidispatch sgi2uapl $F -l$(XO) -w$(XW) -b$(YO) \
	-h$(YW) -p$(PW) | lpr -Plw2; rm $F; }&:tc=sgi_apl:
