# Fits Library

$checkout libpkg.a ../
$update   libpkg.a
$checkin  libpkg.a ../
$exit

libpkg.a:
	t_rfits.x		rfits.h rfits.com <error.h> <fset.h>
	fits_read.x		rfits.com rfits.h <error.h> <imhdr.h>
	fits_rheader.x		rfits.com rfits.h <imhdr.h> <imio.h> <mach.h>
	fits_rimage.x		rfits.com rfits.h <imhdr.h> <mach.h> <fset.h>
	fits_rpixels.x		<mii.h> <mach.h> <fset.h>
	t_wfits.x		wfits.com wfits.h <fset.h> <error.h> <mach.h>
	fits_write.x		wfits.com wfits.h <error.h> <mach.h> <imhdr.h> \
				<fset.h>
	fits_wheader.x		wfits.com wfits.h <mach.h> <imhdr.h>
	fits_wimage.x		wfits.com wfits.h <imhdr.h>
	fits_wpixels.x		wfits.h <fset.h> <mach.h>
	fits_cards.x		wfits.com wfits.h <imhdr.h>
	fits_params.x		wfits.h <time.h>
	;
