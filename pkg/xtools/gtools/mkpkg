# Library for the gtools package.

$checkout libxtools.a lib$
$update	  libxtools.a
$checkin  libxtools.a lib$
$exit

libxtools.a:
	gtascale.x	<mach.h> gtools.h
	gtcolon.x	<gset.h> gtools.h
	gtcopy.x	gtools.h
	gtctran.x
	gtcur.x
	gtcur1.x	gtools.h
	gtfree.x	gtools.h
	gtget.x		<gset.h> gtools.h
#	gthelp.x	<error.h>
	gtinit.x	<gset.h> gtools.h
	gtlabax.x	<gset.h> gtools.h
#	gtpage.x	<ttyset.h>
	gtplot.x	<gset.h> gtools.h
	gtvplot.x	<gset.h> gtools.h
	gtset.x		<gset.h> gtools.h
	gtswind.x	<gset.h> gtools.h <mach.h>
	gtwindow.x	gtools.h
	;
