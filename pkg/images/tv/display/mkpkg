	zrcrim.x	zdisplay.h
	zrgbim.x	zdisplay.h
	zrmim.x		zdisplay.h
	zscale.x	<ctype.h> <imhdr.h> <imset.h> <pmset.h> <imio.h>
	zsttim.x	iis.com iis.h <fio.h> <knet.h>
	zwndim.x	zdisplay.h
	;
